      <!-- Data registers -->

        <!-- DREG_HEALTH -->
        <register>
          <name>DREG_HEALTH</name>
          <description>The health register reports the current status of the GPS module and the other sensors on the board. Monitoring the health register is the easiest way to monitor the quality of the GPS lock and to watch for other problems that could affect the behavior of the board.</description>
          <address>0x55</address>
          <size>32</size>
          <access>read-only</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>

          <fields>

            <!-- SATS_USED -->
            <field>
              <name>SATS_USED</name>
              <description>Reports the number of satellites used in the position solution.</description>
              <bitRange>[31:26]</bitRange>
              <dataType>bitField</dataType>
              <access>read-only</access>
            </field>

            <!-- HDOP -->
            <field>
              <name>HDOP</name>
              <description>Reports the horizontal dilution of precision (HDOP) reported by the GPS. The actual HDOP value is equal to the contents of the HDOP bits divided by 10.</description>
              <bitRange>[25:16]</bitRange>
              <dataType>bitField</dataType>
              <conversionScaleFactor>0.1</conversionScaleFactor>
              <access>read-only</access>
            </field>

            <!-- SATS_IN_VIEW -->
            <field>
              <name>SATS_IN_VIEW</name>
              <description>Reports the number of satellites in view.</description>
              <bitRange>[15:10]</bitRange>
              <dataType>bitField</dataType>
              <access>read-only</access>
            </field>

            <!-- OVF -->
            <field>
              <name>OVF</name>
              <description>Overflow bit. This bit is set if the UM7 is attempting to transmit data over the serial port faster than is allowed given the baud-rate. If this bit is set, reduce broadcast rates in the COM_RATES registers.</description>
              <bitRange>[8:8]</bitRange>
              <dataType>bitField</dataType>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>OVF_OFF</name>
                  <description>No overflow registered</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>OVF_ON</name>
                  <description>Overflow registered, reduce broadcast rates</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>

            <!-- MG_N -->
            <field>
              <name>MG_N</name>
              <description>This bit is set if the sensor detects that the norm of the magnetometer measurement is too far away from 1.0 to be trusted. Usually indicates bad calibration, local field distortions, or both.</description>
              <bitRange>[5:5]</bitRange>
              <dataType>bitField</dataType>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>MAG_NORM_OK</name>
                  <description>Magnetometer norm is OK</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>MAG_NORM_WARN</name>
                  <description>Magnetometer norm is not OK</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>

            <!-- ACC_N -->
            <field>
              <name>ACC_N</name>
              <description>This bit is set if the sensor detects that the norm of the accelerometer measurement is too far away from 1G to be used (i.e. during aggressive acceleration or high vibration).</description>
              <bitRange>[4:4]</bitRange>
              <dataType>bitField</dataType>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>ACC_N_OFF</name>
                  <description>No high acceleration / no high vibration detected</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>ACC_N_ON</name>
                  <description>High acceleration / high vibration detected</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>

            <!-- ACCEL -->
            <field>
              <name>ACCEL</name>
              <description>This bit will be set if the accelerometer fails to initialize on startup.</description>
              <bitRange>[3:3]</bitRange>
              <dataType>bitField</dataType>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>ACCEL_STARTUP_OK</name>
                  <description>Accelerometer startup OK</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>ACCEL_STARTUP_ERR</name>
                  <description>Accelerometer startup error</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>

            <!-- GYRO -->
            <field>
              <name>GYRO</name>
              <description>This bit will be set if the rate gyro fails to initialize on startup.</description>
              <bitRange>[2:2]</bitRange>
              <dataType>bitField</dataType>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>GYRO_STARTUP_OK</name>
                  <description>Gyro startup OK</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>GYRO_STARTUP_ERR</name>
                  <description>Gyro startup error</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>

            <!-- MAG -->
            <field>
              <name>MAG</name>
              <description>This bit will be set if the magnetometer fails to initialize on startup.</description>
              <bitRange>[1:1]</bitRange>
              <dataType>bitField</dataType>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>MAG_STARTUP_OK</name>
                  <description>Magnetometer startup OK</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>MAG_STARTUP_ERR</name>
                  <description>Magnetometer startup error</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>

            <!-- GPS -->
            <field>
              <name>GPS</name>
              <description>This bit is set if the GPS fails to send a packet for more than two seconds. If a GPS packet is ever received, this bit is cleared.</description>
              <bitRange>[0:0]</bitRange>
              <dataType>bitField</dataType>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>GPS_GETTING_PACKETS</name>
                  <description>GPS packet was received within last 2 seconds</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>GPS_NOT_GETTING_PACKETS</name>
                  <description>GPS packet was not received within last 2 seconds</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>

          </fields>
        </register>

