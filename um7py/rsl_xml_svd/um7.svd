        <!-- CREG_MISC_SETTINGS: Misc. settings -->
        <register>
          <name>CREG_MISC_SETTINGS</name>
          <description>This register contains miscellaneous filter and sensor control options.</description>
          <address>0x08</address>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>

          <fields>
            <!-- PPS -->
            <field>
              <name>PPS</name>
              <description>If set, this bit causes the TX2 pin on the IO Expansion header to be used as the PPS input from an external GPS module. PPS pulses will then be used to synchronize the system clock to UTC time of day.</description>
              <bitRange>[8:8]</bitRange>
              <dataType>bitField</dataType>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>PPS_OFF</name>
                  <description>TX2 not used as PPS input from GPS</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>PPS_ON</name>
                  <description>TX2 used as PPS input from GPS</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>

            <!-- ZG -->
            <field>
              <name>ZG</name>
              <description>If set, this bit causes the device to attempt to measure the rate gyro bias on startup. The sensor must be stationary on startup for this feature to work properly.</description>
              <bitRange>[2:2]</bitRange>
              <dataType>bitField</dataType>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>ZG_OFF</name>
                  <description>NOT measure rate gyro bias on startup</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>ZG_ON</name>
                  <description>Measure rate gyro bias on startup</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>

            <!-- Q -->
            <field>
              <name>Q</name>
              <description>If this bit is set, the sensor will run in quaternion mode instead of Euler Angle mode.</description>
              <bitRange>[1:1]</bitRange>
              <dataType>bitField</dataType>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>EULER_MODE</name>
                  <description>The sensor will run in Euler Angle mode</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>QUATERNION_MODE</name>
                  <description>The sensor will run in quaternion mode</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>

            <!-- MAG -->
            <field>
              <name>MAG</name>
              <description>If set, the magnetometer will be used in state updates.</description>
              <bitRange>[0:0]</bitRange>
              <dataType>bitField</dataType>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>MAG_NOT_IN_STATE</name>
                  <description>NOT use magnetometer in state update</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>MAG_IN_STATE</name>
                  <description>Use magnetometer in state update</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>

          </fields>
        </register>

