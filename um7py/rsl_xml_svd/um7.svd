            <field>
              <name>ZERO_GYROS_BIT</name>
              <description>Zero gyros bit</description>
              <bitRange>[0:0]</bitRange>
              <dataType>bitField</dataType>
              <access>write-only</access>
            </field>
          </fields>
        </register>

        <!-- SET_HOME_POSITION -->
        <register>
          <name>SET_HOME_POSITION</name>
          <description>Sets the current GPS latitude, longitude, and altitude as the home position. All future positions will be referenced to the current GPS position.</description>
          <address>0xAE</address>
          <size>32</size>
          <access>write-only</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>

          <fields>
            <!-- SET_HOME_POSITION_BIT -->
            <field>
              <name>SET_HOME_POSITION_BIT</name>
              <description>Set home position bit</description>
              <bitRange>[0:0]</bitRange>
              <dataType>bitField</dataType>
              <access>write-only</access>
            </field>
          </fields>
        </register>

        <!-- SET_MAG_REFERENCE -->
        <register>
          <name>SET_MAG_REFERENCE</name>
          <description>Sets the current yaw heading position as north.</description>
          <address>0xB0</address>
          <size>32</size>
          <access>write-only</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>

          <fields>
            <!-- SET_MAG_REFERENCE_BIT -->
            <field>
              <name>SET_MAG_REFERENCE_BIT</name>
              <description>Set magnetometer reference bit</description>
              <bitRange>[0:0]</bitRange>
              <dataType>bitField</dataType>
              <access>write-only</access>
            </field>
          </fields>
        </register>

        <!-- CALIBRATE_ACCELEROMETERS -->
        <register>
          <name>CALIBRATE_ACCELEROMETERS</name>
          <description>Reboots the board and performs a crude calibration on the accelerometers. Best performed on a flat surface.</description>
          <address>0xB1</address>
          <size>32</size>
          <access>write-only</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>

          <fields>
            <!-- CALIBRATE_ACCELEROMETERS_BIT -->
            <field>
              <name>CALIBRATE_ACCELEROMETERS_BIT</name>
              <description>Calibrate accelerometers bit</description>
              <bitRange>[0:0]</bitRange>
              <dataType>bitField</dataType>
              <access>write-only</access>
            </field>
          </fields>
        </register>

        <!-- RESET_EKF -->
        <register>
          <name>RESET_EKF</name>
          <description>Resets the EKF sensor fusion algorithm.</description>
          <address>0xB3</address>
          <size>32</size>
          <access>write-only</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>

          <fields>
            <!-- RESET_EKF_BIT -->
            <field>
              <name>RESET_EKF_BIT</name>
              <description>Reset EKF bit</description>
              <bitRange>[0:0]</bitRange>
              <dataType>bitField</dataType>
              <access>write-only</access>
            </field>
          </fields>
        </register>

      </registers>
    </peripheral>

    <!-- Hidden Configuration Registers -->
    <peripheral>
      <name>HIDDEN_REGISTER_MAP</name>
      <version>0.1</version>
      <description>Hidden register map for the UM7 board</description>
      <baseAddress>0x00000000</baseAddress>
      <size>32</size>
      <access>read-write</access>

      <addressBlock>
        <offset>0</offset>
        <size>0x500</size>
        <usage>registers</usage>
      </addressBlock>

      <registers>

      <!-- HIDDEN_GYRO_VARIANCE -->
        <register>
          <name>HIDDEN_GYRO_VARIANCE</name>
          <description>Gyro variance</description>
          <address>0x00</address>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
          <fields>
            <!-- GYRO_VARIANCE -->
            <field>
              <name>GYRO_VARIANCE</name>
              <description>Gyro variance for EKF</description>
              <bitRange>[31:0]</bitRange>
              <dataType>float</dataType>
              <access>read-write</access>
            </field>
          </fields>
        </register>

        <!-- HIDDEN_ACCEL_VARIANCE -->
        <register>
          <name>HIDDEN_ACCEL_VARIANCE</name>
          <description>Accelerometer variance</description>
          <address>0x01</address>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>

          <fields>
            <!-- ACCEL_VARIANCE -->
            <field>
              <name>ACCEL_VARIANCE</name>
              <description>Accelerometer variance IEEE floating point value.</description>
              <bitRange>[31:0]</bitRange>
              <dataType>float</dataType>
              <access>read-write</access>
            </field>
          </fields>
        </register>
      </registers>
    </peripheral>

  </peripherals>
</device>
